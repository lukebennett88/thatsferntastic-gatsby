{
  "name": "thatsferntastic.com.au",
  "private": true,
  "description": "Website for thatsferntastic.com.au",
  "author": "Luke Bennett <hello@lukebennett.com.au>",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build:serve": "yarn build && yarn serve",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop --host=0.0.0.0",
    "format": "prettier \"**/*.{md,mdx,json,yaml,js,jsx}\" --write",
    "eslint": "eslint --color --ext .html,.js,.json,.jsx,.md,.ts,.tsx *.* src",
    "eslint:fix": "npm run eslint -- --fix",
    "serve": "gatsby serve --host=0.0.0.0 --open",
    "start": "netlify dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@headlessui/react": "^0.3.1",
    "@reach/auto-id": "^0.13.2",
    "@reach/combobox": "^0.13.2",
    "@reach/dialog": "^0.13.2",
    "@reach/router": "^1.3.4",
    "@react-icons/all-files": "^4.1.0",
    "@sanity/block-content-to-react": "^2.0.7",
    "babel-plugin-react-icons": "^0.1.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^3.6.2",
    "gatsby": "^3.0.0-next.7",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-google-gtag": "^3.0.0",
    "gatsby-plugin-image": "^1.0.0-next.7",
    "gatsby-plugin-react-helmet": "^4.0.0-next.0",
    "gatsby-plugin-sharp": "^3.0.0-next.7",
    "gatsby-transformer-sharp": "^3.0.0-next.1",
    "isomorphic-fetch": "^3.0.0",
    "lodash.isequal": "^4.5.0",
    "match-sorter": "^6.1.0",
    "path": "^0.12.7",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.14.2",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.31.0",
    "react-spring": "^8.0.27",
    "shopify-buy": "^2.11.0",
    "typeface-courier-prime": "^1.1.13",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "eslint-config-adjunct": "^4.9.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-auto": "^0.6.6",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^28.0.2",
    "gatsby-plugin-canonical-urls": "^3.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-netlify": "^3.0.0",
    "gatsby-plugin-postcss": "^4.0.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sitemap": "^3.0.0-next.0",
    "gatsby-source-filesystem": "^3.0.0-next.2",
    "gatsby-source-sanity": "^6.0.5",
    "gatsby-source-shopify-experimental": "^1.11.7",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.7",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "tailwindcss-filters": "^3.0.0",
    "tailwindcss-line-clamp": "^1.0.5"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "auto"
    ],
    "rules": {
      "arrow-body-style": 0,
      "eslint-comments/disable-enable-pair": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/no-noninteractive-tabindex": 0,
      "no-secrets/no-secrets": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/no-array-index-key": 1,
      "react/no-danger": 1,
      "react/no-unescaped-entities": 0,
      "react/no-unused-prop-types": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "react/require-default-props": 0,
      "scanjs-rules/identifier_localStorage": 0,
      "scanjs-rules/property_localStorage": 0,
      "sonarjs/no-duplicate-string": 1,
      "unicorn/no-array-reduce": 0,
      "unicorn/no-new-array": 0,
      "unicorn/no-useless-undefined": 0,
      "unicorn/prefer-spread": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukebennett88/thatsferntastic.com.au"
  },
  "bugs": {
    "url": "https://github.com/lukebennett88/thatsferntastic.com.au/issues"
  }
}
