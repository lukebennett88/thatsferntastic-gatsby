{
  "name": "thatsferntastic.com.au",
  "private": true,
  "description": "Website for thatsferntastic.com.au",
  "author": "Luke Bennett <hello@lukebennett.com.au>",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build:serve": "yarn build && yarn serve",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "develop": "gatsby develop --host=0.0.0.0",
    "dev": "ntl dev",
    "format": "prettier './src/**/*.{md,mdx,json,yaml,js,jsx,ts,tsx}' --write",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint --color --ext .js,.json,.jsx,.ts,.tsx src",
    "serve": "gatsby serve",
    "start": "ntl dev"
  },
  "dependencies": {
    "@headlessui/react": "^0.3.2",
    "@reach/auto-id": "^0.14.0",
    "@reach/combobox": "^0.14.0",
    "@reach/dialog": "^0.14.0",
    "@reach/router": "^1.3.4",
    "@sanity/block-content-to-react": "^2.0.7",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^4.1.2",
    "gatsby": "^3.2.1",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-canonical-urls": "^3.2.0",
    "gatsby-plugin-google-gtag": "^3.2.0",
    "gatsby-plugin-image": "^1.2.0",
    "match-sorter": "^6.3.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.0.0",
    "react-icons": "^4.2.0",
    "react-intersection-observer": "^8.31.0",
    "shopify-buy": "^2.11.0",
    "typeface-courier-prime": "^1.1.13",
    "typeface-montserrat": "^1.1.13"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/jit": "^0.1.18",
    "@tailwindcss/typography": "^0.4.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-adjunct": "^4.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-auto": "^0.7.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-constructor-bind": "^2.0.4",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-no-unsanitized": "^3.1.4",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-scanjs-rules": "^0.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.6.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^29.0.0",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-netlify": "^3.2.0",
    "gatsby-plugin-postcss": "^4.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-plugin-sitemap": "^3.2.0",
    "gatsby-plugin-typescript": "^3.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-source-sanity": "^7.0.2",
    "gatsby-source-shopify": "^4.2.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "postcss": "^8.2.9",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.4",
    "tailwindcss-filters": "^3.0.0",
    "tailwindcss-line-clamp": "^1.0.5",
    "typescript": "^4.2.3"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "eslintConfig": {
    "extends": [
      "auto"
    ],
    "root": true,
    "rules": {
      "@react/jsx-one-expression-per-line": 0,
      "@typescript-eslint/ban-ts-comment": 1,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-assignment": 0,
      "@typescript-eslint/no-unsafe-call": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unused-vars": 1,
      "@typescript-eslint/no-use-before-define": 0,
      "arrow-body-style": 0,
      "eslint-comments/disable-enable-pair": 0,
      "function-paren-newline": 0,
      "implicit-arrow-linebreak": 0,
      "import/prefer-default-export": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/no-noninteractive-tabindex": 0,
      "no-secrets/no-secrets": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "object-curly-newline": 0,
      "react/jsx-filename-extension": 0,
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-wrap-multilines": 0,
      "react/no-array-index-key": 1,
      "react/no-danger": 1,
      "react/no-unescaped-entities": 0,
      "react/no-unused-prop-types": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "react/require-default-props": 0,
      "scanjs-rules/assign_to_src": 0,
      "scanjs-rules/identifier_localStorage": 0,
      "scanjs-rules/property_localStorage": 0,
      "sonarjs/no-duplicate-string": 1,
      "unicorn/no-array-reduce": 0,
      "unicorn/no-new-array": 0,
      "unicorn/no-useless-undefined": 0,
      "unicorn/prefer-spread": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukebennett88/thatsferntastic.com.au"
  },
  "bugs": {
    "url": "https://github.com/lukebennett88/thatsferntastic.com.au/issues"
  }
}
